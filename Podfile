pod 'FontAwesomeIconFactory'

# Debuggin/Analytics
pod 'CocoaLumberjack', '~> 2.0.0-beta4'

# CoreData
pod 'Kipu'

# pod 'ARAnalytics'

end


target :"GM TaplistTests" do

